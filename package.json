  "dependencies": {
    "babel-polyfill": "6.3.14",
    "connected-react-router": "^4.3.0",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "react-redux": "^4.4.1",
    "react-router-dom": "^4.2.2",
    "redux": "^3.3.1",
    "redux-saga": "^0.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "6.4.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.0.4",
